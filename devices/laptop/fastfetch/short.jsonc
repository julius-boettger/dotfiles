{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "data",
        "source": "  \\\\  \\\\ //\n ==\\\\__\\\\/ //\n   //   \\\\//\n==//     //==\n //\\\\___//\n// /\\\\  \\\\==\n  // \\\\  \\\\"
    },
    "display": {
        "size": {
            "binaryPrefix": "si"
        },
        "percent": {
            "type": 1,
            "color": {
                "green": "white",
                "yellow": "white",
                "red": "white"
            }
        }
    },
    "modules": [
        "title",
        {
            "type": "os",
            "format": "{3}"
        },
        {
            "type": "kernel",
            "format": "{2}"
        },
        "wm",
        {
            "type": "shell",
            "format": "{3}"
        },
        {
            "type": "battery",
            "format": "{4}",
            "key": "Battery"
        },
        {
            "type": "colors",
            "symbol": "circle"
        }
    ]
}